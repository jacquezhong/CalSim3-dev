!!!  NDD to CHO minimum flows

define ndd_cho_min {select flow
                  from   ndd_cho_min
                  where  month=month}

define MF_STN021 {alias ndd_cho_min kind 'flow-min-required' units 'cfs'}

goal meet_nddmin {C_STN021_mif < MF_STN021}
